<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT"/>
    </Appenders>

    <Loggers>
        <!-- standard loggers: controlled using 'STD_LOG_LEVEL=debug' -->
        <Root level="info">
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>
</Configuration>
